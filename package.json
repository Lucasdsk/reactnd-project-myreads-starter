{
  "dependencies": {
    "prop-types": "^15.5.8",
    "proptypes": "^1.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "react-scripts": "^1.0.14",
    "react-test-renderer": "15.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
